{
  "name": "feetch",
  "version": "0.0.0",
  "description": "KodeFox fetching library for REST API",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "lib/",
    "src/",
    "!**/__tests__"
  ],
  "author": "KodeFox",
  "license": "MIT",
  "scripts": {
    "prepare": "bob build",
    "build": "yarn prepare",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit -p .",
    "jest": "jest --coverage --passWithNoTests",
    "test": "yarn lint && yarn typecheck && yarn format:check && yarn jest"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "runtypes": "^4.0.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-fetching-library": ">=1.5.0"
  },
  "devDependencies": {
    "@react-native-community/bob": "0.8.0",
    "@testing-library/react-hooks": "3.4.2",
    "@types/jest": "26.0.9",
    "@types/react": "16.9.53",
    "eslint": "7.0.0",
    "eslint-config-kodefox": "0.2.2",
    "fetch-mock": "9.9.0",
    "jest": "26.1.0",
    "node-fetch": "2.6.1",
    "prettier": "2.0.5",
    "react": "16.12.0",
    "react-fetching-library": "1.5.14",
    "react-test-renderer": "16.12.0",
    "ts-jest": "26.2.0",
    "typescript": "3.9.7"
  },
  "eslintConfig": {
    "extends": "kodefox/react"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
