{
  "npm": {
    "publish": true
  },
  "github": {
    "release": true,
    "releaseName": "Naviflex ${version}"
  },
  "git": {
    "pushRepo": "upstream",
    "requireUpstream": false,
    "commitMessage": "Release naviflex ${version}",
    "tagAnnotation": "Naviflex ${version}",
    "tagName": "naviflex-${version}",
    "changelog": "git log --pretty=format:\"* %s (%h)\" ${latestTag}...HEAD | grep -F \"naviflex:\""
  }
}
